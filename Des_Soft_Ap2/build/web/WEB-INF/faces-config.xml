<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>vaiParaPaisListagem</from-outcome>
                <to-view-id>/PaisListagem.xhtml</to-view-id>
            </navigation-case>
            <navigation-case>
                <from-outcome>vaiParaEnderecoListagem</from-outcome>
                <to-view-id>/EnderecoListagem.xhtml</to-view-id>
            </navigation-case>
            <navigation-case>
                <from-outcome>vaiParaCidadeListagem</from-outcome>
                <to-view-id>/CidadeListagem.xhtml</to-view-id>
            </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisListagem.xhtml</from-view-id>
            <navigation-case>
                <from-outcome>voltaParaIndex</from-outcome>
                <to-view-id>/index.xhtml</to-view-id>
            </navigation-case>
            <navigation-case>
                <from-outcome>vaiParaPaisIncluir</from-outcome>
                <to-view-id>/PaisIncluir.xhtml</to-view-id>
            </navigation-case>
            <navigation-case>
                <from-outcome>vaiParaPaisEditar</from-outcome>
                <to-view-id>/PaisEditar.xhtml</to-view-id>
            </navigation-case>
            <navigation-case>
                <from-outcome>vaiParaPaisDetalhe</from-outcome>
                <to-view-id>/PaisDetalhe.xhtml</to-view-id>
            </navigation-case>
            <navigation-case>
                <from-outcome>refresh</from-outcome>
                <to-view-id>/PaisListagem.xhtml</to-view-id>
            </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>paisController</managed-bean-name>
        <managed-bean-class>frb.edu.br.sergionovais.controladores.PaisController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/PaisIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaPaisListagem</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaPaisListagem</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/PaisDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaPaisListagem</from-outcome>
            <to-view-id>/PaisListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaPaisEditar</from-outcome>
            <to-view-id>/PaisEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/EnderecoListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEnderecoIncluir</from-outcome>
            <to-view-id>/EnderecoIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEnderecoDetalhe</from-outcome>
            <to-view-id>/EnderecoDetalhe.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEnderecoEditar</from-outcome>
            <to-view-id>/EnderecoEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>enderecoController</managed-bean-name>
        <managed-bean-class>frb.edu.br.sergionovais.controladores.EnderecoController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cidadeController</managed-bean-name>
        <managed-bean-class>frb.edu.br.sergionovais.controladores.CidadeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/CidadeListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidadeIncluir</from-outcome>
            <to-view-id>/CidadeIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidadeDetalhe</from-outcome>
            <to-view-id>/CidadeDetalhe.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaCidadeEditar</from-outcome>
            <to-view-id>/CidadeEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CidadeIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaCidadeListagem</from-outcome>
            <to-view-id>/CidadeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CidadeDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaCidadeListagem</from-outcome>
            <to-view-id>/CidadeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/CidadeEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaCidadeListagem</from-outcome>
            <to-view-id>/CidadeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/EnderecoIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaEnderecoListagem</from-outcome>
            <to-view-id>/EnderecoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/EnderecoDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaEnderecoListagem</from-outcome>
            <to-view-id>/EnderecoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/EnderecoEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaEnderecoListagem</from-outcome>
            <to-view-id>/EnderecoListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
